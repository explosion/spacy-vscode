{
  "name": "spacy-extension",
  "description": "spaCy support for .cfg files",
  "author": "Explosion AI",
  "repository": "https://github.com/explosion/spacy-vscode",
  "license": "MIT",
  "version": "0.2.0",
  "publisher": "Explosion AI",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "extensionDependencies": [
    "ms-python.python"
  ],
  "activationEvents": [
    "workspaceContains:*.cfg"
  ],
  "contributes": {
    "commands": [
      {
        "command": "spacy_test",
        "title": "spaCy Test"
      },
      {
        "command": "spacy-extension.pythonInterpreter",
        "title": "Return Python Interpreter",
        "category": "spaCy-Extension"
      },
      {
        "command": "spacy-extension.restartServer",
        "title": "Restart Language Server",
        "category": "spaCy-Extension"
      }
    ],
    "configuration": {
      "title": "spaCy Server Configuration",
      "properties": {
        "spacy-extension.defaultPythonInterpreter": {
          "scope": "resource",
          "type": "string",
          "default": "",
          "description": "Specify which Python Interpreter should be used to start the spaCy Extension Server. Please make sure that the selected environment contains all required modules (pygls, spacy). If this setting is empty (by default), the spaCy Extension will use the selected Python Interpreter from your current workspace."
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "16.11.43",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "typescript": "^4.4.4",
    "compare-versions": "^6.0.0-rc.1"
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  }
}